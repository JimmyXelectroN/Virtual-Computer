 /*
 * 4 Way 16 Bit Multiplexor
 */

CHIP Mux4Way16 {
    IN a[16], b[16], c[16], d[16], sel[2];
    OUT out[16];

    PARTS:
    Mux16 (a[0..15]=a, b[0..15]=b, sel=sel[0], out[0..15]=mux1);
    Mux16 (a[0..15]=c, b[0..15]=d, sel=sel[0], out[0..15]=mux2);
    Mux16 (a[0..15]=mux1, b[0..15]=mux2, sel=sel[1], out[0..15]=out);
}
