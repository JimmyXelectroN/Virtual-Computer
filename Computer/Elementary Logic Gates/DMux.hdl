 /*
 * DMux gate:
 * Implementaion of
 * a= And (Or (in, sel), Not sel)
 * b= And (Or (in, sel), sel)
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Not (a=sel, out=notSelA);
    Or (a=in, b=sel, out=orInSelA);
    And (a=orInSelA, b=notSelA, out=a);
    Not (a=sel, out=notSelB);
    Or (a=in, b=notSelB, out=orInSelB);
    And (a=orInSelB, b=sel, out=b);
}
